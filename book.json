{
    "title": "machine-learning-introduction",
    "description": "machine learning notes",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./styles/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "author": "pruce",
    "plugins": [
        "back-to-top-button",
        "splitter",
        "expandable-chapters-small",
        "-sharing",
        "search-pro",
